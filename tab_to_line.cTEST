    int y;
    int i;

    y = 0;
    i = 0;

    if (!(line = (t_point *)malloc(sizeof(t_point) * max_x * max_y)))
       return (-1);
        while (y <= max_y)
        {
            line = tab[y];
            line + max_x;
        }
    y++;
